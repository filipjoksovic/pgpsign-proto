{
    "manifest_version": 2,
    "name": "pgpsign",
    "version": "1.0",
    "description": "Prototype pgp signer",
    "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/beastify",
    "icons": {
        "48": "icons/pgpsign-48.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "browser_action": {
        "default_icon": "icons/pgpsign-48.png",
        "default_title": "PGP Sign",
        "default_popup": "main.html"
    },
    "web_accessible_resources": []
}